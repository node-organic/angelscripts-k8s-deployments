{
  "extends": ["standard", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": ["jest"],
  "rules": {
    "comma-dangle": [0],
    "camelcase": [0]
  },
  "globals": {
    "jest": true,
    "expect": true,
    "beforeAll": true,
    "before": true,
    "after": true,
    "afterAll": true,
    "test": true
  }
}
